<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane fx:id="runAlgoGridPane" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
          minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.171"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="il.ac.mta.zuli.evolution.ui.runningAlgorithm.RunAlgoController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="30.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="30.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <ProgressBar id="taskProgressBar" fx:id="progressBar" maxWidth="1.7976931348623157E308" progress="0.0"
                     GridPane.columnIndex="1" GridPane.columnSpan="5" GridPane.rowIndex="1"/>
        <Button fx:id="bestSolutionButton" mnemonicParsing="false" text="Current Best Solution" textAlignment="CENTER"
                wrapText="true" GridPane.columnIndex="7" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets/>
            </GridPane.margin>
        </Button>
        <FlowPane fx:id="stridesFlowPane" prefHeight="200.0" prefWidth="200.0"
                  style="-fx-border-color: #A0B3AA; -fx-background-color: #FFF9F7;" GridPane.columnIndex="6"
                  GridPane.columnSpan="2" GridPane.rowIndex="4" GridPane.rowSpan="5"/>
        <Label text="Generation Strides:" GridPane.columnIndex="6" GridPane.columnSpan="2" GridPane.rowIndex="3"/>
        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="7.0" GridPane.columnIndex="1"
              GridPane.rowIndex="2">
            <children>
                <Button fx:id="pauseResumeButton" mnemonicParsing="false" text="Pause"/>
                <Button fx:id="stopButton" mnemonicParsing="false" text="Stop"/>
            </children>
            <GridPane.margin>
                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
            </GridPane.margin>
        </HBox>
        <VBox fx:id="changeDescriptorBox" alignment="TOP_CENTER" style="-fx-border-color: #442526;"
              GridPane.columnIndex="1" GridPane.columnSpan="4" GridPane.rowIndex="4" GridPane.rowSpan="5"/>
    </children>
</GridPane>
