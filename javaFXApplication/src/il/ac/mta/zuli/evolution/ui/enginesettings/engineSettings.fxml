<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="il.ac.mta.zuli.evolution.ui.enginesettings.EngineSettingsController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <VBox alignment="TOP_CENTER" spacing="5.0">
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
        <HBox alignment="CENTER" spacing="10.0">
            <Label text="Show: "/>
            <ChoiceBox fx:id="engineDisplayChoiceBox" prefWidth="150.0"/>
            <Label fx:id="populationLabel" text="population x"/>
        </HBox>
        <Label fx:id="selectedDetailsLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
               text="nothing to see here" wrapText="true"/>
    </VBox>
</ScrollPane>
